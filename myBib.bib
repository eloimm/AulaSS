  author = {Sánchez-Valdivia, Nacho and Pérez-del-Pulgar, Carmen and family=Bont, given=Jeroen, prefix=de, useprefix=true and Anguelovski, Isabelle and López-Gay, Antonio and Pistillo, Andrea and Triguero-Mas, Margarita and Duarte-Salles, Talita},
