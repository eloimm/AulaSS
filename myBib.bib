@article{perryMeasuringPublicService1996,
  title = {Measuring {{Public Service Motivation}}: {{An Assessment}} of {{Construct Reliability}} and {{Validity}}},
  shorttitle = {Measuring {{Public Service Motivation}}},
  author = {Perry, James L.},
  date = {1996},
  journaltitle = {Journal of Public Administration Research and Theory: J-PART},
  volume = {6},
  number = {1},
  pages = {5--22},
  issn = {1053-1858},
  url = {http://www.indiana.edu/~jlpweb/papers/measuring%20psm_perry_JPART1996.pdf},
  abstract = {The public administration literature makes many assertions that the motivations of individuals who pursue public service careers differ in important ways from other members of American society. This research advances the study of these assertions by creating a scale to measure public service motivation. Public service motivation (PSM) represents an individual's predisposition to respond to motives grounded primarily or uniquely in public institutions. The construct is associated conceptually with six dimensions: attraction to public policy making, commitment to the public interest, civic duty, social justice, self-sacrifice, and compassion. Likert-type items are developed for each dimension to create the PSM scale. The measurement theory for the scale is tested using confirmatory factor analysis (CFA). The present study reports initial reliability and validity results.}
}

@report{perssonFamilyRupturesStress2016,
  type = {Working Paper},
  title = {Family {{Ruptures}}, {{Stress}}, and the {{Mental Health}} of the {{Next Generation}}},
  author = {Persson, Petra and Rossin-Slater, Maya},
  date = {2016-05},
  number = {22229},
  institution = {{National Bureau of Economic Research}},
  url = {http://www.nber.org/papers/w22229},
  urldate = {2016-06-30},
  abstract = {This paper studies how in utero exposure to maternal stress from family ruptures affects later mental health. We find that prenatal exposure to the death of a maternal relative increases take-up of ADHD medications during childhood and anti-anxiety and depression medications in adulthood. Further, family ruptures during pregnancy depress birth outcomes and raise the risk of perinatal complications necessitating hospitalization. Our results suggest large welfare gains from preventing fetal stress from family ruptures and possibly from economically induced stressors such as unemployment. They further suggest that greater stress exposure among the poor may partially explain the intergenerational persistence of poverty.}
}

