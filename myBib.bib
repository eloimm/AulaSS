@misc{europeancommissionChangesChildFamily2018,
  title = {Changes in Child and Family Policies in the {{EU28}} in 2017 : {{European}} Platform for Investing in Children : Annual Thematic Report.},
  shorttitle = {Changes in Child and Family Policies in the {{EU28}} in 2017},
  author = {European Commission and RAND Europe},
  date = {2018-08-03},
