@article{fineDependenceIndependenceInterdependence2005,
  title = {Dependence, Independence or Inter-Dependence? {{Revisiting}} the Concepts of ‘Care’ and ‘Dependency’},
  shorttitle = {Dependence, Independence or Inter-Dependence?},
  author = {Fine, Michael and Glendinning, Caroline},
  date = {2005-07},
  journaltitle = {Ageing \&amp; Society},
  volume = {25},
  number = {4},
  pages = {601--621},
  issn = {1469-1779, 0144-686X},
  doi = {10.1017/S0144686X05003600},
  url = {https://www.cambridge.org/core/journals/ageing-and-society/article/dependence-independence-or-interdependence-revisiting-the-concepts-of-care-and-dependency/1E3F5F0B1A90329E07DC20A4FA4665E8},
  urldate = {2017-05-03},
  abstract = {Research and theory on ‘dependency’ and ‘care-giving’ have to date proceeded along largely separate lines, with little sense that they are exploring and explaining different aspects of the same phenomenon. Research on ‘care’, initially linked to feminism during the early 1980s, has revealed and exposed to public gaze what was hitherto assumed to be a ‘natural’ female activity. Conversely, disability activists and writers who have promoted a social model of disability have seen the language of and the policy focus upon ‘care’ as oppressive and objectifying. ‘Dependency’ is an equally contested concept: sociologists have scrutinised the social construction of dependency; politicians have ascribed negative connotations of passivity; while medical and social policy discourse employs the term in a positivist sense as a measure of physical need for professional intervention. Autonomy and independence, in contrast, are promoted as universal and largely unproblematic goals. These contrasting perspectives have led social theory, research and policies to separate and segregate the worlds of ‘carers’ from those for whom they ‘care’. Drawing on the work of Kittay and others, this paper explores the ways in which sociological perspectives can develop new understanding of the social contexts of ‘care’ and ‘dependence’.},
  keywords = {ageing,care,dependency,Disability,independence,inter-dependence},
  file = {C\:\\Users\\mayordomome\\Zotero\\storage\\M9P2429G\\1E3F5F0B1A90329E07DC20A4FA4665E8.html}
}

@article{fingermanHelicopterParentsLanding2012,
  title = {Helicopter {{Parents}} and {{Landing Pad Kids}}: {{Intense Parental Support}} of {{Grown Children}}},
  shorttitle = {Helicopter {{Parents}} and {{Landing Pad Kids}}},
  author = {Fingerman, Karen L. and Cheng, Yen-Pi and Wesselmann, Eric D. and Zarit, Steven and Furstenberg, Frank and Birditt, Kira S.},
  date = {2012-08-01},
  journaltitle = {Journal of Marriage and Family},
  volume = {74},
  number = {4},
  pages = {880--896},
  issn = {1741-3737},
  doi = {10.1111/j.1741-3737.2012.00987.x},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1741-3737.2012.00987.x/abstract},
  urldate = {2016-06-28},
  abstract = {Popular media describe adverse effects of helicopter parents who provide intense support to grown children, but few studies have examined implications of such intense support. Grown children (N = 592, M age = 23.82 years, 53\% female, 35\% members of racial/ethnic minority groups) and their parents (N = 399, M age = 50.67 years, 52\% female; 34\% members of racial/ethnic minority groups) reported on the support they exchanged with one another. Intense support involved parents' providing several types of support (e.g., financial, advice, emotional) many times a week. Parents and grown children who engaged in such frequent support viewed it as nonnormative (i.e., too much support), but grown children who received intense support reported better psychological adjustment and life satisfaction than grown children who did not receive intense support. Parents who perceived their grown children as needing too much support reported poorer life satisfaction. The discussion focuses on generational differences in the implications of intense parental involvement during young adulthood.},
  langid = {english},
  keywords = {family,family relations,Helicopter Parents,intergenerational relationships,parent,social support,young adulthood},
  file = {C\:\\Users\\mayordomome\\Zotero\\storage\\DIM36EZC\\abstract.html}
}

