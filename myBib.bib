@report{commanderInformeSobreDesarrollo1997,
  title = {Informe sobre el desarrollo mundial 1997 : el estado en un mundo en transformacion},
  shorttitle = {Informe sobre el desarrollo mundial 1997},
  author = {Commander, Simon John and Weder, Beatrice Silvia and Pradhan, Sanjay and Chhibber, Ajay and Fuhr, Harald L. and Kane, Cheikh T. and Leechor, Chad and Evans, Alison Margaret and Levy, Brian David},
  date = {1997-08-01},
  number = {17300},
