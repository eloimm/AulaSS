@article{chorpitaAssessmentSymptomsDSMIV2000,
  title = {Assessment of Symptoms of {{DSM-IV}} Anxiety and Depression in Children: A Revised Child Anxiety and Depression Scale},
  shorttitle = {Assessment of Symptoms of {{DSM-IV}} Anxiety and Depression in Children},
  author = {Chorpita, B. F. and Yim, L. and Moffitt, C. and Umemoto, L. A. and Francis, S. E.},
