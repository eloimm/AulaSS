@article{almeidaImpactCOVID19Households2021,
  title = {The Impact of {{COVID-19}} on Households´ Income in the {{EU}}},
  author = {Almeida, Vanda and Barrios, Salvador and Christl, Michael and De Poli, Silvia and Tumino, Alberto and family=Wielen, given=Wouter, prefix=van der, useprefix=true},
