@thesis{navarrovarasCuidadoPrimeraInfancia2019,
  type = {phdthesis},
  title = {El cuidado de la primera infancia. Desigualdades sociales y territoriales en la metrópolis de Barcelona},
  author = {Navarro Varas, Lara},
  date = {2019-11-06},
  journaltitle = {TDX (Tesis Doctorals en Xarxa)},
  institution = {{Universitat Autònoma de Barcelona}},
  url = {http://www.tdx.cat/handle/10803/669604},
  urldate = {2020-11-06},
  abstract = {En un contexto marcado por la creciente importancia de la educación y la atención de la temprana infancia como uno de los instrumentos de la política social más relevantes para combatir la pobreza y la desigualdad, esta tesis doctoral se centra en el análisis del cuidado en España en la última década y en su reescalado a nivel local. En este sentido, el principal objetivo de la disertación pasa por contrastar el uso diferencial de los diferentes recursos de cuidado para los niños/as menores de tres años en el período 2006-2016 así como por examinar el papel de lo público en la reproducción y en la acentuación de éste. La aproximación analítica se realiza desde dos dimensiones diferentes: desde el ‘uso’ que las familias realizan de los diferentes modelos de cuidado y desde la ‘oferta’ pública de servicios de educación y atención para la primera infancia acotada al área metropolitana de Barcelona. En ambas aproximaciones adquieren relevancia las aportaciones de naturaleza empírica, constituyendo el tratamiento de datos de naturaleza administrativa un recurso novedoso y con elevado potencial respecto a la investigación previa en esta materia. Los resultados obtenidos muestran que la desfamiliarización del cuidado de la primera infancia en España constituye un recurso mediado por la clase económica, estando sobrerrepresentados los niños que pertenecen a los estratos sociales superiores de la sociedad española. Lo era antes de la crisis y lo sigue siendo con más intensidad en el contexto actual. Ante el limitado sistema de licencias parentales, la falta de una prestación económica universal por hijo a cargo y la escasez de servicios, los existentes no están diseñados para que las familias de menores recursos se beneficien más. La descentralización de los servicios a las comunidades autónomas y el alto grado de autonomía trasladado a su vez a la administración local, perfila diferentes modelos que no son ajenos a la igualdad de oportunidades. La heterogeneidad de la oferta pública entre los municipios metropolitanos dibuja un escenario diverso al que son más sensibles los infantes con menores recursos económicos. El municipio de residencia condiciona extraordinariamente el acceso a los servicios de titularidad pública dirigidos a la primera infancia. Mientras las familias con mayor capacidad adquisitiva se muestran más resilientes a la diversidad territorial, las situadas en la cola inferior de la distribución son mucho más vulnerables. Los argumentos ofrecidos pueden resultar de interés en la formulación de políticas en la escala estatal y en la escala local/metropolitana.},
  isbn = {9788449091377},
  langid = {spanish},
  keywords = {31,Área metropolitana de Barcelona,Àrea metropolitana de Barcelona,Ciències Socials,Desigualdad social,Desigualtat social,Family policies,Metropolitan area of Barcelona,Políticas familiares,Polítiques familiars,Social inequality},
  annotation = {Accepted: 2020-09-25T15:04:02Z},
  file = {C\:\\Users\\mayordomome\\Zotero\\storage\\WJ7Q9CTF\\EL CUIDADO DE LA PRIMERA INFANCIA.pdf;C\:\\Users\\mayordomome\\Zotero\\storage\\9PDXYL46\\669604.html}
}

